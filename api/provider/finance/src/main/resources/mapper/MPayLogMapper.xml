<mapper namespace="com.yrsoft.finance.dao.MPayLogDao">

    <resultMap type="com.yrsoft.finance.entity.MPayLog" id="MPayLogResult">
	    <result property="id"  column="id" javaType="String"/>
	    <result property="outTradeNo"  column="out_trade_no" javaType="String"/>
	    <result property="memberId"  column="member_id" javaType="String"/>
	    <result property="payType"  column="pay_type" javaType="Integer"/>
	    <result property="payMoney"  column="pay_money" javaType="Long"/>
	    <result property="payTime"  column="pay_time" javaType="Integer"/>
	    <result property="payState"  column="pay_state" javaType="Integer"/>
	    <result property="notifyTime"  column="notify_time" javaType="Integer"/>
	    <result property="notifyContent"  column="notify_content" javaType="String"/>
	    <result property="costType"  column="cost_type" javaType="Integer"/>
	    <result property="payAccount"  column="pay_account" javaType="String"/>
	    <result property="paySerialCode"  column="pay_serial_code" javaType="String"/>
