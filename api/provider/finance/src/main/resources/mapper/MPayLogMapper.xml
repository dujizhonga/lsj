	<sql id="selectMPayLogVo">
        select id, out_trade_no, member_id, pay_type, pay_money, pay_time, pay_state, notify_time, notify_content, cost_type, pay_account, pay_serial_code from f_pay_log
    </sql>
	

    <select id="selectMPayLogById" parameterType="String" resultMap="MPayLogResult">
        <include refid="selectMPayLogVo"/>
        where id = #{id}
    </select>

	<select id="selectMPayLogList" parameterType="MPayLog"
		resultMap="MPayLogResult">
		<include refid="selectMPayLogVo" />
		<where>
			<if test="id != null  and id != '' "> and id = #{id}</if>
			<if test="outTradeNo != null  and outTradeNo != '' "> and out_trade_no = #{outTradeNo}</if>
			<if test="memberId != null  and memberId != '' "> and member_id = #{memberId}</if>
			<if test="payType != null "> and pay_type = #{payType}</if>
			<if test="payMoney != null "> and pay_money = #{payMoney}</if>
			<if test="payTime != null "> and pay_time = #{payTime}</if>
			<if test="payState != null "> and pay_state = #{payState}</if>
			<if test="notifyTime != null "> and notify_time = #{notifyTime}</if>
			<if test="notifyContent != null  and notifyContent != '' "> and notify_content = #{notifyContent}</if>
			<if test="costType != null "> and cost_type = #{costType}</if>
			<if test="payAccount != null  and payAccount != '' "> and pay_account = #{payAccount}</if>
			<if test="paySerialCode != null  and paySerialCode != '' "> and pay_serial_code = #{paySerialCode}</if>
		</where>
		order by pay_time DESC
	</select>
        
    <insert id="insertMPayLog" parameterType="MPayLog">
        insert into f_pay_log
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="id != null  and id != ''  ">id,</if>
			<if test="outTradeNo != null  and outTradeNo != ''  ">out_trade_no,</if>
			<if test="memberId != null  and memberId != ''  ">member_id,</if>
			<if test="payType != null  ">pay_type,</if>
			<if test="payMoney != null  ">pay_money,</if>
			<if test="payTime != null  ">pay_time,</if>
			<if test="payState != null  ">pay_state,</if>
			<if test="notifyTime != null  ">notify_time,</if>
			<if test="notifyContent != null  and notifyContent != ''  ">notify_content,</if>
			<if test="costType != null  ">cost_type,</if>
			<if test="payAccount != null  and payAccount != ''  ">pay_account,</if>
			<if test="paySerialCode != null  and paySerialCode != ''  ">pay_serial_code,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="id != null  and id != ''  ">#{id},</if>
			<if test="outTradeNo != null  and outTradeNo != ''  ">#{outTradeNo},</if>
			<if test="memberId != null  and memberId != ''  ">#{memberId},</if>
			<if test="payType != null  ">#{payType},</if>
			<if test="payMoney != null  ">#{payMoney},</if>
			<if test="payTime != null  ">#{payTime},</if>
			<if test="payState != null  ">#{payState},</if>
			<if test="notifyTime != null  ">#{notifyTime},</if>
			<if test="notifyContent != null  and notifyContent != ''  ">#{notifyContent},</if>
			<if test="costType != null  ">#{costType},</if>
			<if test="payAccount != null  and payAccount != ''  ">#{payAccount},</if>
			<if test="paySerialCode != null  and paySerialCode != ''  ">#{paySerialCode},</if>
         </trim>
    </insert>
	 
    <update id="updateMPayLog" parameterType="MPayLog">
        update f_pay_log
       <set>
            <if test="outTradeNo != null  and outTradeNo != ''  ">out_trade_no = #{outTradeNo},</if>
            <if test="memberId != null  and memberId != ''  ">member_id = #{memberId},</if>
            <if test="payType != null  ">pay_type = #{payType},</if>
            <if test="payMoney != null  ">pay_money = #{payMoney},</if>
            <if test="payTime != null  ">pay_time = #{payTime},</if>
            <if test="payState != null  ">pay_state = #{payState},</if>
            <if test="notifyTime != null  ">notify_time = #{notifyTime},</if>
            <if test="notifyContent != null  and notifyContent != ''  ">notify_content = #{notifyContent},</if>
            <if test="costType != null  ">cost_type = #{costType},</if>
            <if test="payAccount != null  and payAccount != ''  ">pay_account = #{payAccount},</if>
            <if test="paySerialCode != null  and paySerialCode != ''  ">pay_serial_code = #{paySerialCode},</if>
       </set>
        where id = #{id}
    </update>


    <update id="updateMPayLogByIds"  parameterType="java.util.Map">
        UPDATE f_pay_log
        <set>
           <if test="outTradeNo != null  and outTradeNo != ''  ">out_trade_no = #{outTradeNo},</if>
           <if test="memberId != null  and memberId != ''  ">member_id = #{memberId},</if>
