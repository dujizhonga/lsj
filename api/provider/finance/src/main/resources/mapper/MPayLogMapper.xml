           <if test="costType != null  ">cost_type = #{costType},</if>
           <if test="payAccount != null  and payAccount != ''  ">pay_account = #{payAccount},</if>
           <if test="paySerialCode != null  and paySerialCode != ''  ">pay_serial_code = #{paySerialCode},</if>
        </set>
        WHERE  id in
        <foreach item="id" collection="ids" open="(" separator="," close=")">
            #{id}
        </foreach>


    </update>

	
    <delete id="deleteMPayLogByIds" parameterType="String">
        delete from f_pay_log where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
