<mapper namespace="com.yrsoft.member.dao.MOwnerDriverDao">

    <resultMap type="com.yrsoft.member.entity.MOwnerDriver" id="MOwnerDriverResult">
    <result property="id"  column="id" javaType="String"/>
    <result property="ownerId"  column="owner_id" javaType="String"/>
    <result property="driverId"  column="driver_id" javaType="String"/>
    <result property="createTime"  column="create_time" javaType="Integer"/>
    <result property="deleteFlag"  column="delete_flag" javaType="Integer"/>
    <result property="agree"  column="agree" javaType="Integer"/>
