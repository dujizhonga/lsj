<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yrsoft.position.dao.PoCarPositionLogDao">

    <resultMap type="com.yrsoft.position.entity.PoCarPositionLog" id="PoCarPositionLogResult">
    <result property="id"  column="id" javaType="String"/>
    <result property="carId"  column="car_id" javaType="String"/>
    <result property="lng"  column="lng" javaType="String"/>
    <result property="lat"  column="lat" javaType="String"/>
    <result property="time"  column="time" javaType="Integer"/>
    </resultMap>
	
	<sql id="selectPoCarPositionLogVo">
        select id, car_id, lng, lat, time from po_car_position_log
    </sql>
	

    <select id="selectPoCarPositionLogById" parameterType="String" resultMap="PoCarPositionLogResult">
        <include refid="selectPoCarPositionLogVo"/>
        where id = #{id}
    </select>

    <select id="selectPoCarPositionLogList" parameterType="PoCarPositionLog" resultMap="PoCarPositionLogResult">
        <include refid="selectPoCarPositionLogVo"/>
        <where>
                            <if test="id != null  and id != '' "> and id = #{id}</if>
                            <if test="carId != null  and carId != '' "> and car_id = #{carId}</if>
                            <if test="lng != null  and lng != '' "> and lng = #{lng}</if>
                            <if test="lat != null  and lat != '' "> and lat = #{lat}</if>
                            <if test="time != null "> and time = #{time}</if>
                    </where>
    </select>
        
    <insert id="insertPoCarPositionLog" parameterType="PoCarPositionLog">
        insert into po_car_position_log
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="id != null  and id != ''  ">id,</if>
			<if test="carId != null  and carId != ''  ">car_id,</if>
			<if test="lng != null  and lng != ''  ">lng,</if>
			<if test="lat != null  and lat != ''  ">lat,</if>
			<if test="time != null  ">time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="id != null  and id != ''  ">#{id},</if>
			<if test="carId != null  and carId != ''  ">#{carId},</if>
			<if test="lng != null  and lng != ''  ">#{lng},</if>
			<if test="lat != null  and lat != ''  ">#{lat},</if>
			<if test="time != null  ">#{time},</if>
         </trim>
    </insert>
	 
    <update id="updatePoCarPositionLog" parameterType="PoCarPositionLog">
        update po_car_position_log
       <set>
            <if test="carId != null  and carId != ''  ">car_id = #{carId},</if>
            <if test="lng != null  and lng != ''  ">lng = #{lng},</if>
            <if test="lat != null  and lat != ''  ">lat = #{lat},</if>
            <if test="time != null  ">time = #{time},</if>
       </set>
        where id = #{id}
    </update>


    <update id="updatePoCarPositionLogByIds"  parameterType="java.util.Map">
        UPDATE po_car_position_log
        <set>
           <if test="carId != null  and carId != ''  ">car_id = #{carId},</if>
           <if test="lng != null  and lng != ''  ">lng = #{lng},</if>
           <if test="lat != null  and lat != ''  ">lat = #{lat},</if>
           <if test="time != null  ">time = #{time},</if>
        </set>
        WHERE  id in
        <foreach item="id" collection="ids" open="(" separator="," close=")">
            #{id}
        </foreach>


    </update>

	
    <delete id="deletePoCarPositionLogByIds" parameterType="String">
        delete from po_car_position_log where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
</mapper>